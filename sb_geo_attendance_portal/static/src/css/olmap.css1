.o_geofence_container{
    width: 100%;    
    min-height: 450px;
    height: calc(100vh - 132px);
}
.o_geofence_draw_container{
    width: 100%;
    min-height: 450px;
    height: calc(100vh - 400px);
}
.gmap_kisok_container{
    margin-top: 10px;
    min-height: 50px;
}
.gmap_kisok_container h3{
    padding:2px 2px 2px 2px;
    color: #4c4c4c;
}
.gmap_kisok_view{
    width: 100%;
    height: 200px;
    padding-top: 3px;
}
.gmap_kisok_toggle{
    width: 100%;
    right: 20px;
    color: #555555;
    cursor: pointer;
    border-top: 1px dashed #adb5bd;
    border-bottom: 1px dashed #adb5bd;    
    padding: 5px 5px 5px 5px;
    background-color: #9e8b921f;
}
.gmap_kisok_toggle::before{
    position: absolute;
    right: 10px;
}
.gmap_clear_button{
    background-color: white; 
    margin-bottom: 5px;
    padding: 1px 8px 2px 8px;
} 
.ol-clear {
    top: 72px;
    left: .5em;
}
.ol-draw{
    top: 94px;
    left: .5em;
}
